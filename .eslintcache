 useUpdateStore({ type: types.USERS_UPDATE })\r\n\r\n    const usersRef = useRef();\r\n    useEffect(() => {\r\n        usersRef.current = props.users;\r\n    })\r\n\r\n    const handleGetUsers = () => {\r\n        props.dispatch(getUsers());\r\n    }\r\n\r\n    const handleSendPageParams = () => {\r\n        const {\r\n            page,\r\n            size,\r\n        } = usersRef.current;\r\n        props.dispatch(getLimitedUsers({ page, size }));\r\n    }\r\n\r\n    useEffect(() => {\r\n        handleGetUsers();\r\n        // handleSendPageParams()\r\n    }, [])\r\n\r\n    const handleCloseModal = () => {\r\n        updateStore({\r\n            isOpenModalEdit: false,\r\n            isOpenModalAdd: false,\r\n        })\r\n    }\r\n\r\n    const handleChangeName = (e) => {\r\n        updateStore({\r\n            name: e.target.value,\r\n        })\r\n    }\r\n    const handleChangeLocation = (e) => {\r\n        updateStore({\r\n            location: e.target.value,\r\n        })\r\n    }\r\n    const handleChangeAge = (e) => {\r\n        updateStore({\r\n            age: e.target.value,\r\n        })\r\n    }\r\n\r\n    const handleSearch = (e) => {\r\n        updateStore({\r\n            search: e.target.value,\r\n        })\r\n    }\r\n\r\n    const handleSendSearch = async () => {\r\n        await props.dispatch(searchUser(search));\r\n    }\r\n\r\n    const user = {\r\n        name,\r\n        location,\r\n        age,\r\n        id,\r\n    }\r\n\r\n    const handleAddUser = async () => {\r\n        await props.dispatch(addUser(user));\r\n        handleGetUsers();\r\n        handleCloseModal();\r\n    }\r\n\r\n    const handleDeleteRow = async (row) => {\r\n        const {\r\n            id,\r\n        } = row;\r\n        await props.dispatch(deleteUser(id));\r\n        handleGetUsers()\r\n    }\r\n\r\n    const handleEditUser = async () => {\r\n        const editedUser = {\r\n            id: currentUser.id,\r\n            name: name || currentUser.name,\r\n  